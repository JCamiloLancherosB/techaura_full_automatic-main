{
  "name": "techaura-intelligent-bot",
  "type": "commonjs",
  "version": "2.0.0",
  "description": "TechAura Intelligent Bot with AI-powered routing and personalization",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx watch --tsconfig tsconfig.json src/app.ts",
    "start": "tsx --tsconfig tsconfig.json src/app.ts",
    "build": "tsc -p tsconfig.json",
    "build:quick": "tsc --skipLibCheck --noEmitOnError false --allowJs",
    "build:watch": "tsc --watch --skipLibCheck --noEmitOnError false",
    "clean": "rimraf dist",
    "test": "echo \"No tests specified\" && exit 0",
    "test:mysql": "node test-mysql-config.js",
    "test:notificador": "node test-notificador.js",
    "test:notificador:shell": "bash test-notificador-integration.sh",
    "lint": "eslint \"src/**/*.ts\"",
    "start:prod": "pnpm run build && node --max-old-space-size=512 --expose-gc dist/app.js",
    "prod": "node --max-old-space-size=512 --expose-gc dist/app.js",
    "verify": "tsx src/scripts/verifyIntegrity.ts",
    "migrate": "npx knex migrate:latest --knexfile knexfile.js",
    "migrate:rollback": "npx knex migrate:rollback --knexfile knexfile.js",
    "migrate:status": "npx knex migrate:status --knexfile knexfile.js"
  },
  "keywords": [
    "whatsapp",
    "bot",
    "ai",
    "intelligent",
    "personalization",
    "ecommerce",
    "usb",
    "music"
  ],
  "author": "TechAura Team",
  "license": "ISC",
  "dependencies": {
    "@builderbot/bot": "^1.3.5",
    "@builderbot/database-mysql": "^1.3.5",
    "@builderbot/provider-baileys": "^1.3.5",
    "@google/generative-ai": "^0.24.1",
    "axios": "^1.13.1",
    "better-sqlite3": "^12.4.1",
    "cohere-ai": "^7.19.0",
    "dotenv": "^16.6.1",
    "exceljs": "^4.4.0",
    "express": "^4.21.2",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "mammoth": "^1.11.0",
    "multer": "^2.0.2",
    "mysql2": "^3.15.3",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^5.23.2",
    "papaparse": "^5.4.1",
    "pdf-parse": "^2.4.5",
    "sharp": "^0.34.4",
    "socket.io": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "twilio": "^5.10.4",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/express": "^4.17.21",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.19.30",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.14",
    "@types/socket.io": "^3.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "eslint-plugin-builderbot": "latest",
    "nodemon": "^3.1.10",
    "rimraf": "^5.0.10",
    "rollup": "^4.52.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCamiloLancherosB/techaura_full_automatic.git"
  },
  "bugs": {
